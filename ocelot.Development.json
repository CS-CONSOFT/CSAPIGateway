{
  "Routes": [
    {
      "UpstreamPathTemplate": "/api/{everything}",
      "DownstreamPathTemplate": "/api/{everything}",
      "DownstreamScheme": "http",
      "DownstreamHostAndPorts": [
        {
          "Host": "api3.heei.com.br",
          "Port": 80 //a porta interna que ta rodando o container
        }
      ],
      "SwaggerKey": "basico"
    },
    {
      "UpstreamPathTemplate": "/csst/{everything}",
      "DownstreamPathTemplate": "/csst/{everything}",
      "DownstreamScheme": "http",
      "DownstreamHostAndPorts": [
        {
          "Host": "api1.heei.com.br",
          "Port": 80
        }
      ],
      "SwaggerKey": "estatica"
    }
  ],
  "SwaggerEndPoints": [
    {
      "Key": "basico",
      "Config": [
        {
          "Name": "API Básico",
          "Version": "v1",
          "Url": "https://api3.heei.com.br/swagger/v1/swagger.json"
        }
      ]
    },
    {
      "Key": "estatica",
      "Config": [
        {
          "Name": "API Estática",
          "Version": "v1",
          "Url": "https://api1.heei.com.br/swagger/v1/swagger.json"
        }
      ]
    }
  ]


}
